<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
'-//Hibernate/Hibernate Mapping DTD 3.0//EN'
'http://hibernate.sourceforge.net /hibernate-mapping-3.0.dtd'>

<hibernate-mapping>
	<class name="pers.mjw.Blog" table="blog">
		<id name="name" column="name" type="java.lang.String">
		</id>
		<property name="head" column="head" type="java.lang.String"></property>
		<property name="text" column="text" type="java.lang.String"></property>
		<property name="date" column="date" type="java.lang.String"></property>
	</class>
</hibernate-mapping>